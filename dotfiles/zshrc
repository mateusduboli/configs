ZSH=$HOME/.oh-my-zsh
ZSH_THEME="candy"
ZSH_CUSTOM="$HOME/.zsh"
ZSH_CHAORDIC="$HOME/.zshrc-chaordic"
APP_BIN="$HOME/Applications/bin"
plugins+=(git rails vagrant mercurial brew pip django ruby)


source $ZSH/oh-my-zsh.sh

### Source chaordic environment
if [ -e $ZSH_CHAORDIC ]; then
	source $ZSH_CHAORDIC
fi

for file in `ls $APP_BIN/*`; do
	. $file
done

export PATH="/usr/local/mysql/bin:$PATH"
export CFLAGS="-Qunused-arguments"
export CPPFLAGS="-Qunused-arguments"
